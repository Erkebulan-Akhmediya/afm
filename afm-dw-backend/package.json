{
  "name": "dw",
  "author": "Dreamlab Technologies Ltd",
  "license": "ISC",
  "version": "1.0.0",
  "description": "DW Back End",
  "main": "hr.ts",
  "scripts": {
    "start:dev": "ts-node --files src/hr.ts",
    "start:nodemon": "npx nodemon --files src/hr.ts",
    "start": "NODE_OPTIONS=--max-old-space-size=2048 node ./dist/src/hr.js",
    "build": "rimraf ./dist && tsc --project ./ && copyfiles ./src/config/*.json ./src/nacl.json ./.env ./dist && cd ./dist/src/soap && mkdir wsdl && copyfiles ../../../src/soap/wsdl/*.wsdl ./wsdl &&  cd ../ && mkdir templates && copyfiles ../../src/templates/*.html ./templates && cp ../../src/utils/ru.morpher.min.js ./utils/ru.morpher.min.js",
    "create-migration": "db-migrate create %npm_config_file% --config src/config/database.json -e $npm_config_schema -m migrations/$npm_config_dir",
    "migrate-up": "db-migrate up:$npm_config_dir --config src/config/database.json -e $npm_config_schema",
    "migrate-down": "db-migrate down:$npm_config_dir --config src/config/database.json -e $npm_config_schema",
    "migrate-up-all": "db-migrate up:all --config src/config/database.json -e db",
    "migrate-reset": "db-migrate down:all --config src/config/database.json -e db",
    "test": "jest --forceExit",
    "clean": "node unComment.js"
  },
  "keywords": [
    "Express",
    "Postgre"
  ],
  "dependencies": {
    "@chelmsfordbeer/pdf-creator-node": "^2.2.5",
    "@types/pdfkit": "^0.12.3",
    "activedirectory": "^0.7.2",
