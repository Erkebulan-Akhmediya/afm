    "nodemailer": "^6.7.5",
    "pdf-creator-node": "^2.2.4",
    "pdfkit": "^0.13.0",
    "pg": "^8.6.0",
    "puppeteer": "^17.1.3",
    "request-ip": "^2.1.3",
